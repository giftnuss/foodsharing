{
  "name": "foodsharing-frontend",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint": "bin/eslint -c package.json",
    "lint:markdown": "yarn run lint-markdown",
    "lint-markdown": "markdownlint --config ../.markdownlint.json",
    "fix": "bin/eslint -c package.json --fix",
    "clean-assets": "shx rm -rf ../assets",
    "build": "yarn clean-assets && NODE_ENV=production webpack",
    "dev": "yarn clean-assets && webpack serve --hot --config serve.config.js",
    "test": "node test"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@sentry/vue": "~5.30.0",
    "@sum.cumo/vue-datepicker": "^3.1.0",
    "autosize": "^4.0.2",
    "bootstrap": "^4.6.0",
    "bootstrap-vue": "~2.18.1",
    "copy-webpack-plugin": "^6.4.1",
    "core-js": "^3.10.1",
    "corejs-typeahead": "^1.3.1",
    "cropperjs": "^1.5.11",
    "date-fns": "^2.20.1",
    "install": "^0.13.0",
    "jquery": "^3.4.1",
    "jquery-contextmenu": "^2.9.2",
    "jquery-jcrop": "^0.9.13",
    "jquery-migrate": "^3.3.2",
    "jquery-slimscroll": "^1.3.8",
    "jquery-ui": "<1.12.0-beta.1",
    "jquery.ui.position": "^1.11.4",
    "leaflet": "^1.7.1",
    "leaflet.awesome-markers": "^2.0.5",
    "leaflet.markercluster": "^1.5.0",
    "lodash.pickby": "^4.6.0",
    "mapbox-gl": "<2.0.0",
    "mapbox-gl-leaflet": "^0.0.14",
    "markdown-it": "^12.0.4",
    "node-sass": "~4.14.1",
    "object-path": "^0.11.5",
    "serviceworker-webpack-plugin": "^1.0.1",
    "socket.io-client": "^2.3.1",
    "tinysort": "~3.2.7",
    "twemoji": "^13.0.2",
    "typeahead-address-photon": "^1.1.0",
    "typeahead-addresspicker": "^1.0.1",
    "typeface-alfa-slab-one": "^1.1.13",
    "underscore": "^1.13.0",
    "vue": "~2.6.12",
    "vue-croppie": "^2.0.1",
    "vue-infinite-loading": "^2.4.5",
    "vue-simple-suggest": "^1.11.1",
    "vue-slider-component": "~3.2.11",
    "vue-tel-input": "^4.4.2",
    "vuelidate": "^0.7.6",
    "whatwg-fetch": "^3.6.2"
  },
  "devDependencies": {
    "@babel/core": "^7.13.15",
    "@babel/preset-env": "^7.13.15",
    "@sentry/webpack-plugin": "~1.15.0",
    "@vue/test-utils": "~1.1.3",
    "babel-eslint": "^10.1.0",
    "babel-loader": "~8.2.2",
    "css-loader": "^3.6.0",
    "dotenv": "^8.2.0",
    "eslint": "~7.24.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-html": "~6.1.2",
    "eslint-plugin-import": "~2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-vue": "^7.8.0",
    "exports-loader": "^0.7.0",
    "expose-loader": "^0.7.5",
    "file-loader": "~6.2.0",
    "glob": "^7.1.6",
    "imports-loader": "^0.8.0",
    "jsdom": "^16.5.2",
    "jsdom-global": "^3.0.2",
    "json-loader": "^0.5.7",
    "loader-utils": "^2.0.0",
    "markdownlint-cli": "^0.28.1",
    "mini-css-extract-plugin": "^1.4.1",
    "mkdirp": "^1.0.4",
    "mocha": "^8.3.2",
    "node-localstorage": "^2.1.6",
    "null-loader": "^4.0.1",
    "sass-loader": "^9.0.3",
    "shx": "^0.3.3",
    "sinon": "^10.0.1",
    "style-loader": "^1.3.0",
    "url-loader": "^4.1.1",
    "vue-eslint-parser": "^7.6.0",
    "vue-loader": "~15.9.6",
    "vue-template-compiler": "~2.6.12",
    "webpack": "~4.44.2",
    "webpack-bundle-analyzer": "~4.4.0",
    "webpack-cli": "~4.6.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "~5.7.3",
    "write-file-webpack-plugin": "~4.5.1",
    "yaml-loader": "^0.6.0"
  },
  "eslintConfig": {
    "parser": "vue-eslint-parser",
    "parserOptions": {
      "parser": "babel-eslint"
    },
    "env": {
      "browser": true
    },
    "extends": [
      "standard",
      "plugin:vue/recommended"
    ],
    "rules": {
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "object-curly-spacing": [
        "error",
        "always"
      ],
      "vue/no-v-html": [
        "off"
      ]
    },
    "overrides": [
      {
        "files": [
          "*.test.js"
        ],
        "env": {
          "mocha": true
        }
      }
    ]
  },
  "eslintIgnore": [
    "test/_compiled.js"
  ]
}
